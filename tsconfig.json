{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@api/*": ["src/model/api/*"],
      "@store/*": ["src/view-model/store/*"],
      "@classes/*": ["src/model/classes/*"],
      "@constants/*": ["src/model/constants/*"],
      "@data/*": ["src/model/data/*"],
      "@types/*": ["src/model/types/*"]
      /* "@assets/*": [
        "src/assets/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@scenes/*": [
        "src/scenes/*"
      ],
      "@theme/*": [
        "src/theme/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ] */
    }
  }
}
